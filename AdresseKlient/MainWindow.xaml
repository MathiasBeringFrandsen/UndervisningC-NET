<Window x:Class="AdresseKlient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdresseKlient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="VejListe" HorizontalAlignment="Left" Height="302" Margin="0,19,0,0" VerticalAlignment="Top" Width="184" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Button x:Name="GetButton" Content="Hent" HorizontalAlignment="Left" Margin="473,56,0,0" VerticalAlignment="Top" Click="GetButton_Click"/>
        <TextBox x:Name="Vejnavn" HorizontalAlignment="Left" Margin="227,19,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Husnummer" HorizontalAlignment="Left" Margin="383,19,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
