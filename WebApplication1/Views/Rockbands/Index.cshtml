@{
}

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>RockBands</title>
</head>
<body>
    <div>
        @{
            int i = 1;
            foreach (string band in ViewBag.Bands)
            {
                <div> @i. @band</div>
                i++;
            }
        }
        
    </div>
</body>
</html>
